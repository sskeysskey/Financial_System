{
    "manifest_version": 3,
    "name": "网页表格数据抓取器",
    "version": "1.0",
    "description": "从当前页面抓取特定表格数据，并将其导出为CSV文件。",
    "permissions": [
        "activeTab",
        "scripting",
        "downloads"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {}
    },
    "icons": {}
}