{
    "manifest_version": 3,
    "name": "TE Multi Scraper",
    "version": "1.0",
    "description": "一次性抓取 Bonds/Commodities/Currencies/Economics/Indices 共五类数据并下载 CSV",
    "permissions": [
        "tabs",
        "scripting",
        "downloads",
        "storage"
    ],
    "host_permissions": [
        "https://tradingeconomics.com/*"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}